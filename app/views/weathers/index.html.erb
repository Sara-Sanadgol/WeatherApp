<h1>Weathers#index</h1>
<p>Find me in app/views/weathers/index.html.erb</p>



<table>
  <tr>
    <th>City</th>
    <th>Temp</th>
    <th>minTemp</th>
    <th>maxTemp</th>
  </tr>


  <% @weather.each do |w| %>
  <tr>
    <td><%= w.city %></td>
    <td><%= w.temp %></td>
    <td><%= w.minTemp %></td>
    <td><%= w.maxTemp %></td>
  </tr>
  <% end %>

</table>
